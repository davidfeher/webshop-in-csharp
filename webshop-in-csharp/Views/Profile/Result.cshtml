@model IEnumerable<WebshopInCsharp.Models.ProfileSearchResultModel>

@{
    ViewData["Title"] = "Result";
}

<h2>Search results</h2>
 

@foreach (var item in Model) {

    <div class="panel panel-default">
        <div class="panel panel-heading">
            <h3 class="panel-title">@Html.DisplayFor(modelItem => item.DisplayName) - @Html.DisplayFor(modelItem => item.Age)</h3>
        </div>   
    
    <div class="panel-body"></div>
        <div class="row">
            <div class="col-md-8">
                @Html.DisplayFor(modelItem => item.Description)
            </div>
            <div class="col-md-4">
                <img src="~/uploads/@item.ProfilePicture" class="profilePicture" />
            </div>
        </div>
     </div>
}
